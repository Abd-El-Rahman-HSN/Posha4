
<%= simple_form_for(@individual) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :hyper %>
    <%= f.input :photo %>
    <%= f.input :linkd %>
    <%= f.association :player %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
