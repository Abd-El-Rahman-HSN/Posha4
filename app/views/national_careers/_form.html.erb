
<%= simple_form_for(@national_career) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :team %>
    <%= f.input :matches %>
    <%= f.input :goals %>
    <%= f.input :assists %>
    <%= f.input :yellow %>
    <%= f.input :red %>
    <%= f.association :player %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
