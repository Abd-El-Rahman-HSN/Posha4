<p id="notice"><%= notice %></p>

<h1>Nationals</h1>

<table>
  <thead>
    <tr>
      <th>Competition</th>
      <th>Countts</th>
      <th>Years</th>
      <th>Position</th>
      <th>Player</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nationals.each do |national| %>
      <tr>
        <td><%= national.competition %></td>
        <td><%= national.countts %></td>
        <td><%= national.years %></td>
        <td><%= national.position %></td>
        <td><%= national.player %></td>
        <td><%= link_to 'Show', national %></td>
        <td><%= link_to 'Edit', edit_national_path(national) %></td>
        <td><%= link_to 'Destroy', national, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New National', new_national_path %>
