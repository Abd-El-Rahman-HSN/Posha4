
<%= simple_form_for(@national) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :competition %>
    <%= f.input :countts %>
    <%= f.input :years %>
    <%= f.input :position %>
    <%= f.association :player %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
