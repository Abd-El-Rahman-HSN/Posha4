<!DOCTYPE html>
<html>
  <head>
    <title>Poshka4</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= stylesheet_link_tag    'owl.carousel.min', media: 'all', 'data-turbolinks-track': 'reload' %>



    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>

    <%= javascript_include_tag 'popper.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'bootstrap.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'owl.carousel.min', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'main', 'data-turbolinks-track': 'reload' %>

  </head>

  <body>
      <%= render 'layouts/nav' %>
      <%= render 'layouts/header' %>
      
    <%= yield %>

      <%= render 'layouts/footer' %>
  </body>
</html>
